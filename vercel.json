{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    
    "routes":  [
    {
     "src": "/(.*)",
     "dest": "/"
    },
    {
      "src": "/preview",
      "dest": "/preview"
    },
    {
      "src": "/download",
      "dest": "/download"
    },
    {
      "src": "/upload",
      "dest": "/upload"
    }
  ]
  }
  