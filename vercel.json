{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    
    "routes":  [
    {
     "src": "/",
     "dest": "/src/index.js"
    },
    {
      "src": "/preview",
      "dest": "/src/index.js"
    },
    {
      "src": "/download",
      "dest": "/src/index.js"
    },
    {
      "src": "/upload",
      "dest": "/src/index.js"
    }
    
  ]
  }
  